var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');
var prompt = function(texto) { return lines.shift(); };

let valor  = parseInt(prompt("Insira um valor de 1 at√© 1000"));
    for (let x = 1; x < valor + 1; x++) {
        if (x % 2 !== 0) {
            console.log(x);
        } else if (x < 1 && x > 1000) {
            break;
        }
    }
