var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');


var prompt = function(texto) { return lines.shift(); };

let casos = parseInt(prompt("Numero de casos"));
let a = 1;
let b = 2;
let c = 3;

for (let x = 0; x < casos; x++) {
    console.log(a + " " + b + " " + c + " PUM");
    a = a + 4;
    b = b + 4;
    c = c + 4;
}
