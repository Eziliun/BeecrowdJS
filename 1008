var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');
var prompt = function(texto) { return lines.shift();};

    var numF = parseInt(prompt("Numero do funcionario"))
    var numHT = parseInt(prompt("Horas trabalhadas"))
    var salarioF = parseFloat(prompt("Salario"))
    console.log("NUMBER = " + (numF))
    console.log("SALARY = U$ " + ((numHT*salarioF).toFixed(2)))
